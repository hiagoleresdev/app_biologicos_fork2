{
  "name": "app-biologicos",
  "version": "0.0.1",
  "license": "MIT",
  "homepage": "https://",
  "description": "Pagina para obter informações e prototipação de registro de farmacos biologicos",
  "author": {
    "name": "Vanderson Paim e Camila Sampaio",
    "email": "vandersonsilvapaim@gmail.com"
  },
  "keywords": [
    "ISO",
    "ISO 11238",
    "ISO 11239",
    "ISO 11240",
    "ISO 1161",
    "ISO 11615"
  ],
  "repository": "https://bitbucket.org/justocamila01/app-biologicos/src/master/",
  "private": true,
  "scripts": {
    "ng": "ng",
    "start": "node server",
    "build": "ng build --prod",
    "test": "ng test",
    "update": "ng update @angular/cli @angular/core --next=true",
    "build:p": "npm run build:prod && npm run compression && npm run analyzer && npm run qa:p ",
    "build:pf": "npm run build:prod && npm run compression && npm run qa:p ",
    "build:h": "npm run build:homol && npm run compression && npm run qa:h ",
    "build:pk": "ng-packagr -p package.json --verbose",
    "build:prod": "ng build --base-href /medicamentosbiologicos/ --deploy-url /medicamentosbiologicos/ --prod --verbose",
    "build:prod2": "ng build --base-href / --deploy-url / --prod --verbose",
    "watch": "ng build --watch --output-hashing=all --source-map --preserve-symlinks",
    "qa:p": "npm run compodoc",
    "qa:h": "npm run lint && npm run compodoc",
    "lint": "ng lint app-biologicos --format=stylish",
    "compodoc": "compodoc -p src/tsconfig.app.json -n Documentação --hideGenerator --output dist/app-biologicos/docs/ && gzip-all \"dist/app-biologicos/docs/*.*\" && brotli-all \"dist/app-biologicos/docs/*.*\"",
    "compression": " npm run compression:gzip && npm run compression:brotli && npm run rm-unnecessary",
    "analyzer": "npm run build --prefix /Users/vandersonsilvapaimgmail.com/Documents/workspace/projetos/Medicamentos Biologicos/angular-pwa-app/ && npx rimraf \"dist/app-biologicos/docs/*.gz.br\"",
    "compression:gzip": " gzip-all \"dist/app-biologicos/*.*\" && gzip-all \"dist/app-biologicos/assets/*.*\" && gzip-all \"dist/app-biologicos/assets/mocks/*.*\"",
    "compression:brotli": " brotli-all \"dist/app-biologicos/*.*\" && brotli-all \"dist/app-biologicos/assets/*.*\" && brotli-all \"dist/app-biologicos/assets/mocks/*.*\"",
    "lighthouse:p": "lighthouse --verbose --view --output html --output-path dist/app-biologicos/docs/lighthouse.html https://biologicos.org/",
    "npm-clear-env": "npm config rm proxy && npm config rm https-proxy && npm config --global rm proxy && npm config --global rm https-proxy",
    "update-node": "sudo npm cache clean -f  && sudo npm install -g n && sudo n stable",
    "rm-unnecessary": "npx rimraf \"dist/app-biologicos/*.gz.br\" && npx rimraf \"dist/app-biologicos/assets/*.gz.br\" && npx rimraf \"dist/app-biologicos/assets/mocks/*.gz.br\"",
    "e2e": "ng e2e",
    "ngcc_jspdf": "ngcc --source ./node_modules/jspdf/dist --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "postinstall": "npm run ngcc_jspdf"
  },
  "engines": {
    "node": "16.17.0",
    "npm": "8.15.0"
  },
  "dependencies": {
    "@angular-devkit/build-optimizer": "0.1202.18",
    "@angular/animations": "14.2.9",
    "@angular/cdk": "14.2.6",
    "@angular/common": "14.2.9",
    "@angular/compiler": "14.2.9",
    "@angular/core": "14.2.9",
    "@angular/forms": "14.2.9",
    "@angular/localize": "14.2.9",
    "@angular/material": "14.2.6",
    "@angular/platform-browser": "14.2.9",
    "@angular/platform-browser-dynamic": "14.2.9",
    "@angular/pwa": "14.2.8",
    "@angular/router": "14.2.9",
    "@angular/service-worker": "14.2.9",
    "express": "^4.18.2",
    "ngx-particle": "1.5.0",
    "rxjs": "7.5.7",
    "sass": "1.56.0",
    "zone.js": "0.11.8"
  },
  "devDependencies": {
    "@angular-devkit/architect": "0.1402.8",
    "@angular-devkit/build-angular": "14.2.8",
    "@angular-devkit/core": "14.2.8",
    "@angular-devkit/schematics": "14.2.8",
    "@angular/cli": "14.2.8",
    "@angular/compiler-cli": "14.2.9",
    "@angular/language-service": "14.2.9",
    "@compodoc/compodoc": "1.1.19",
    "@ngtools/webpack": "14.2.8",
    "@schematics/angular": "14.2.8",
    "@schematics/update": "0.901.11",
    "@types/jasmine": "4.3.0",
    "@types/jasminewd2": "2.0.10",
    "@types/jspdf": "1.3.3",
    "brotli-all": "1.0.0",
    "codelyzer": "6.0.2",
    "cypress": "10.11.0",
    "gzip-all": "1.0.0",
    "husky": "8.0.1",
    "jasmine-core": "4.5.0",
    "jasmine-spec-reporter": "7.0.0",
    "json": "11.0.0",
    "jspdf": "2.5.1",
    "karma": "6.4.1",
    "karma-chrome-launcher": "3.1.1",
    "karma-coverage-istanbul-reporter": "3.0.3",
    "karma-jasmine": "5.1.0",
    "karma-jasmine-html-reporter": "2.0.0",
    "lighthouse": "9.6.8",
    "ng-packagr": "14.2.2",
    "protractor": "7.0.0",
    "rimraf": "3.0.2",
    "ts-node": "10.9.1",
    "tslib": "2.4.1",
    "tslint": "6.1.2",
    "typescript": "4.8.4"
  },
  "husky": {
    "hooks": {}
  }
}
